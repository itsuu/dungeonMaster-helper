<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="dndlevel.Controller">
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <HBox>
               <children>
                  <Label text="Dungeon Level">
                     <font>
                        <Font size="26.0" />
                     </font>
                     <padding>
                        <Insets left="15.0" top="5.0" />
                     </padding>
                  </Label>
               </children></HBox>
         </children>
      </VBox>
   </top>
   <bottom>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Label text="Label">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </bottom>
   <left>
      <VBox fx:id="leftVBox" prefHeight="413.0" prefWidth="251.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="ChamberListLabel" prefHeight="28.0" prefWidth="251.0" text="Chamber List">
               <font>
                  <Font size="19.0" />
               </font>
               <padding>
                  <Insets left="70.0" right="70.0" />
               </padding></Label>
            <ListView fx:id="listViewChamberList" prefHeight="467.0" prefWidth="251.0" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="25.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </VBox>
   </left>
   <right>
      <VBox prefHeight="486.0" prefWidth="210.0" BorderPane.alignment="CENTER">
         <children>
            <Label prefHeight="28.0" prefWidth="213.0" text="Door List">
               <font>
                  <Font size="19.0" />
               </font>
               <padding>
                  <Insets left="60.0" right="60.0" />
               </padding></Label>
            <ChoiceBox fx:id="choiceBoxDoorList" prefHeight="25.0" prefWidth="264.0">
               <VBox.margin>
                  <Insets bottom="10.0" left="15.0" right="15.0" />
               </VBox.margin></ChoiceBox>
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="25.0" top="10.0" />
         </BorderPane.margin>
      </VBox>
   </right>
</BorderPane>
